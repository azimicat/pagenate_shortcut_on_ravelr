{
    "manifest_version": 3,
    "name": "Page Navigator",
    "version": "1.0",
    "description": "Use J and K keys to navigate pages on ravelry.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
